#%RAML 1.0 Library
usage: employee data types
types:
  employee-data-types:
    type: object
    properties:
        employeeID:
          required: true
          type: integer
          example: 12345
        employeeName:
          required: true
          type: string
          example: "Huu"
        employeeStatus:
          required: true
          type: string
          example: "A" 